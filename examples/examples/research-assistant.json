{
    "manifest_version": "0.1.0",
    "agent_id": "example.research.assistant",
    "agent_name": "Research Assistant Agent",
    "agent_version": "1.0.0",
    "owner": {
        "type": "organization",
        "identifier": "Example Research Lab"
    },
    "purpose": {
        "primary_code": "information.research",
        "description": "Aggregates and summarizes publicly available information within declared constraints."
    },
    "forbidden_actions": [
        "execute_payments",
        "modify_external_systems",
        "store_personal_data",
        "provide_authoritative_legal_or_financial_advice"
    ],
    "autonomy": {
        "level": 2
    },
    "risk_profile": {
        "level": "medium",
        "notes": "Accesses external sources but performs no irreversible operations."
    },
    "data_handling": {
        "stores_personal_data": false,
        "retention": "none"
    },
    "contact": {
        "email": "research@example.com"
    },
    "language": {
        "primary": "en",
        "supported": [
            "en"
        ]
    }
}

